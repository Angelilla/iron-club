
<h1>Calendar</h1>


<div>
  {{#each activities}} {{!-- ref app.js l-93 --}}
    <ul>
      <li>
        <p>{{title}}</p>
        <a class="" href="/user/activity/{{this.id}}">See More</a>
        <a href="/user/activity/{{this.id}}/edit">EDIT</a>
        <form action="/user/activity/{{this.id}}/book-activity" method="POST">
          <button type="submit" class="btn-profile">JOIN</button>
        </form>
      </li>
    </ul>
  {{/each}}
</div>
